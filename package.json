{
  "name": "express-example-project",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Eder Mariano <edermariano@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel --presets es2015 -d build/ src -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/server.js",

    "predev": "yarn sequelize db:migrate",
    "dev": "nodemon src/server.js",
    "prestart": "yarn sequelize db:migrate",
    "preci:test": "NODE_ENV=test yarn sequelize db:migrate",
    "pretest": "NODE_ENV=test yarn sequelize db:migrate",
    "ci:test": "NODE_ENV=test jest --runInBand",
    "test": "NODE_ENV=test jest --runInBand",
    "test:watch": "jest --watch",
    "docker": "docker exec -it lab-api sh"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.0",
    "sucrase": "^3.10.1",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "date-fns": "^2.0.0-beta.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2",
    "yup": "^0.27.0"
  },
  "engines": {
    "node": "10.x"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "__tests__/factories",
      "__tests__/util"
    ],
    "transform": {
      ".(js|jsx|ts|tsx)": "@sucrase/jest-plugin"
    }
  }
}
